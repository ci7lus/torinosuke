{
    "version": 2,
    "name": "twitter-oauth",
    "routes": [
        {
            "src": "/api(.*)",
            "dest": "src/api.ts"
        },
        {
            "src": "/(.*)",
            "dest": "src/$1"
        }
    ],
    "builds": [
        {
            "src": "src/index.html",
            "use": "now-parcel"
        },
        {
            "src": "src/api.ts",
            "use": "@now/node@canary"
        }
    ],
    "env": {
        "IS_NOW": "true"
    }
}
